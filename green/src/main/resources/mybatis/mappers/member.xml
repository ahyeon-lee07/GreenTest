<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
      PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mapper.member">
	<resultMap id="memResult" type="memberVO">
		<result property="id" column="id" />
        <result property="pw" column="pw" />
        <result property="name" column="name" />
        <result property="birth" column="birth" />
        <result property="phone" column="phone" />
        <result property="email1" column="email1" />
        <result property="email2" column="email2" />
        <result property="zipCode" column="zipCode" />
        <result property="address1" column="address1" />
        <result property="address2" column="address2" />
        <result property="address3" column="address3" />
        <result property="address4" column="address4" />
        <result property="grade" column="grade" />
        <result property="mileage" column="mileage" />
        <result property="masterYN" column="masterYN" />
        <result property="joinDate" column="joinDate" />
	</resultMap> 
	
	<insert id="insertMember"  parameterType="memberVO">
		<![CDATA[
		 insert into member(id,pw,name,birth,phone,zipCode,address1,address2,address3,email1,email2)
		 values(#{id}, #{pw}, #{name}, #{birth}, #{phone}, #{zipCode}, #{address1}, #{address2}, #{address3}, #{email1}, #{email2})
		]]>      
	</insert>
	
</mapper>